{
	"name": "http2js",
	"version": "1.0.0",
	"description": "A fully type-safe HTTP/2 Node.js library using native http2 module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"test:watch": "vitest",
		"example": "ts-node src/example.ts",
		"examples": "ts-node src/examples.ts",
		"dev": "ts-node src/example.ts",
		"test:h2c": "vitest run --reporter=verbose"
	},
	"keywords": [
		"http2",
		"typescript",
		"native",
		"type-safe"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@types/node": "^25.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^6.0.5",
		"vitest": "^4.0.18"
	}
}
